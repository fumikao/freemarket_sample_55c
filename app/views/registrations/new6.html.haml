.k_su_single-container
  %header.k_su_header
    .k_su_header__inner
      %h1.k_su_header__inner--title
        = link_to "#" do
          %img{src: "https://www-mercari-jp.akamaized.net/assets/img/common/common/logo.svg?891577966", alt: "mercari", width: "185", height: "49"}

      .k_su_header__inner--pan
        .k_su_header__inner--pan-li
          会員情報
          .k_su_progress-status.k_su_active-back
            .k_su_progress-status-after1.k_su_active-back
        .k_su_header__inner--pan-li
          電話番号認証
          .k_su_progress-status.k_su_active-back
            .k_su_progress-status-after2.k_su_active-back
        .k_su_header__inner--pan-li
          お届け先住所入力
          .k_su_progress-status.k_su_active-back
            .k_su_progress-status-after3.k_su_active-back
        .k_su_header__inner--pan-li
          支払い方法
          .k_su_progress-status.k_su_active-back
            .k_su_progress-status-after4.k_su_active-back
        .k_su_header__inner--pan-li.k_su_active 
          完了
          .k_su_progress-status.k_su_active-back

  %main.k_su_wrapper6
    %section.k_su_section6
      %h2.k_su_section6__header
        会員登録完了
      .k_su_section6__inner
        .k_su_section6__inner--form
          .k_su_section6__inner--form-sentence
            ありがとうございます。
          .k_su_section6__inner--form-sentence
            会員登録が完了しました。

        .k_su_section6__inner--form2
          .k_su_section6__inner--form2-input
            %input{type: 'submit', value: 'メルカリをはじめる', class: 'k_su_section6__inner--form2-input-btn'}
            
      .k_su_section6__footer
        %img{src: 'https://placehold.jp/700x300.png'}
            
  %footer.k_su_footer
    = render 'footer'
  
  