.n_mypage_center
  %seciton.n_mypage_center__user_icon
    = link_to "#", class: "n_mypage_center__user_icon__link" do
      %img.n_mypage_center__user_icon__avater{src:"https://static.mercdn.net/images/member_photo_noimage_thumb.png"}
      .n_mypage_center__user_icon__name
        = user.nickname
        %ul.n_mypage_center__user_icon__name__list
          %li.n_mypage_center__user_icon__name__list__column
            評価
            %span.n_mypage_center__user_icon__name__list__column__number
              0
          %li.n_mypage_center__user_icon__name__list__column
            出品数
            %span.n_mypage_center__user_icon__name__list__column__number
              1
